<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>vmmc</id>
	<name>Voluntary Male Circumcision Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.vmmc</package>
	<author>KIMANI</author>
	<description>
		Used to enroll patient to VMMC program and generates reports on the same
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/vmmc/update.rdf</updateURL>
	<require_version>1.9.7</require_version>
	<!-- / Module Properties -->

	
	<!-- Required Modules -->
	<require_modules>
																											
		<require_module version="14.2-SNAPSHOT">
			org.openmrs.module.kenyaemr
		</require_module>
			
	</require_modules>
	<!-- / Required Modules -->
	
	<!-- Module Activator -->
	<activator>org.openmrs.module.vmmc.VoluntaryMaleCircumcisionActivator</activator>
	
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with vmmc. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

